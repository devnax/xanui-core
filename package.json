{
   "name": "@xanui/core",
   "version": "1.1.2",
   "description": "",
   "private": false,
   "main": "./cjs/index.js",
   "module": "./index.js",
   "types": "./index.d.ts",
   "type": "module",
   "sideEffects": false,
   "scripts": {
      "start": "makepack start",
      "build": "makepack build",
      "release": "makepack release"
   },
   "dependencies": {
      "oncss": "^1.1.14",
      "pretty-class": "^1.0.4",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "react-state-bucket": "^1.1.8"
   },
   "devDependencies": {
      "@types/react": "^19.0.2",
      "@types/react-dom": "^19.0.2",
      "makepack": "^1.6.8",
      "typescript": "^4.9.5"
   },
   "keywords": [],
   "exports": {
      ".": {
         "require": {
            "types": "./index.d.ts",
            "default": "./cjs/index.js"
         },
         "import": {
            "types": "./index.d.ts",
            "default": "./index.js"
         }
      },
      "./*": {
         "require": {
            "types": "./*.d.ts",
            "default": "./cjs/*.js"
         },
         "import": {
            "types": "./*.d.ts",
            "default": "./*.js"
         }
      },
      "./cjs": null
   }
